{
  "services": [
    {
      "name": "example_service_1",
      "command": "python example_service_1.py",
      "restart_policy": "always",
      "max_retries": 10,
      "retry_delay": 5
    },
    {
      "name": "example_service_2",
      "command": "python example_service_2.py",
      "restart_policy": "on-failure",
      "max_retries": 5,
      "retry_delay": 10
    },
    {
      "name": "failing_service_demo",
      "command": "python failing_script.py",
      "restart_policy": "on-failure",
      "max_retries": 3,
      "retry_delay": 3
    }
  ]
}